<p id="notice"><%= notice %></p>

<h1>Listing Venues</h1>

<div class="table-responsive">
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Name</th>
				<th>Type</th>
				<th colspan="2"></th>
			</tr>
		</thead>

		<tbody>
			<% @venues.each do |venue| %>
			<tr>
				<td><%= link_to venue.name, edit_admin_venue_path(venue) %></td>
				<td><%= venue.type %></td>
				<td><%= link_to 'Show', venue_path(venue) %></td>
				<td><%#= link_to 'Destroy', [:admin, venue], method: :delete, data: { confirm: 'Are you sure?' } %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>

<br>

<%= link_to 'New Venue', new_admin_venue_path %>
